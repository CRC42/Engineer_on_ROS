<launch>
	<arg name="paused" default="false"/>

	<include file="$(find gazebo_ros)/launch/empty_world.launch">
		<arg name="paused" value="$(arg paused)"/>
		<!-- arg name="world_name" value="$(find engineer_gazebo)/worlds/test.world"/-->
	</include>

	<param name="robot_description" command="$(find xacro)/xacro --inorder '$(find sp_description)/urdf/sentry/swerve_sentry_v1/swerve_sentry_v1.urdf.xacro'"/>

	<node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-z 0.5 -x 0.3 -y 0.3 -urdf -model swerve_sentry_v1 -param robot_description"/>

  <node
    name="joint_state_publisher"
    pkg="joint_state_publisher"
    type="joint_state_publisher" />
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find sp_description)/urdf.rviz" />

</launch>
